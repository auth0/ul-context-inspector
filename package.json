{
  "name": "@auth0/ul-context-inspector",
  "version": "1.1.1",
  "description": "Universal Login Inspector tool to inspect Auth0 ACUL context data.",
  "keywords": [
    "auth0",
    "ACUL",
    "inspector"
  ],
  "author": "Auth0",
  "repository": {
    "type": "git",
    "url": "https://github.com/auth0/ul-context-inspector"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.19.0"
  },
  "type": "module",
  "main": "dist/ul-context-inspector.umd.js",
  "module": "dist/ul-context-inspector.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/ul-context-inspector.css",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css",
    "dist/ul-context-inspector.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ul-context-inspector.es.js",
      "require": "./dist/ul-context-inspector.umd.js"
    },
    "./style.css": {
      "default": "./dist/ul-context-inspector.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch",
    "prepare": "npm run build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "preview": "vite preview",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react": "^18.3.1 || ^19.0.0",
    "react-dom": "^18.3.1 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.40",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.0",
  "vite": "^7.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.8"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "prismjs": "^1.30.0",
    "react-simple-code-editor": "^0.13.1",
    "tailwind-merge": "^3.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
