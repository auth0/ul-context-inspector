{
  "name": "ul-context-inspector",
  "version": "0.1.0",
  "description": "React UI component to inspect context data exposed on the window object.",
  "keywords": [
    "react",
    "inspector",
    "context",
    "window",
    "debug",
    "vite",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "main": "dist/ul-context-inspector.cjs.js",
  "module": "dist/ul-context-inspector.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/style.css",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css",
    "dist/style.css",
    "dist/assets/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ul-context-inspector.es.js",
      "require": "./dist/ul-context-inspector.cjs.js"
    },
    "./style.css": {
      "default": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && ls -1 dist",
    "watch": "vite build --watch",
    "prepare": "npm run build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "preview": "vite preview",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^24.5.2",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.40",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.6.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@auth0/quantum-product": "^2.8.1",
    "@base-ui-components/react": "^1.0.0-beta.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "prismjs": "^1.30.0",
    "react-simple-code-editor": "^0.13.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  }
}
